<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="details">
		<field
			name="al_id"
			type="hidden"
		/>
		<field
			name="al_art"
			type="hidden"
		/>
		<field
			name="published"
			type="list"
			id="published"
			class="inputbox"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			size="1"
			default="1"
			filter="integer">
			<option	value="1">JPUBLISHED</option>
			<option	value="0">JUNPUBLISHED</option>
			<option	value="2">JARCHIVED</option>
			<option	value="-2">JTRASHED</option>
		</field>
		<field
			name="ordering"
			type="hidden"
		/>
		<field
			name="al_field"
			type="LinkFields"
			class="inputbox"
			label="COM_MAMS_DRILLDOWN_FIELD_FIELD_LABEL"
			description="COM_MAMS_DRILLDOWN_FIELD_FIELD_DESC"
		/>
		<field
			name="al_link"
			type="ArticleLinks"
			class="inputbox"
			label="COM_MAMS_ARTLINK_FIELD_LINK_LABEL"
			description="COM_MAMS_ARTLINK_FIELD_LINK_DESC"
		/>
	</fieldset>
</form>

